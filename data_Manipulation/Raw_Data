import yfinance as yf
import pandas as pd
import os


os.makedirs("../data/Raw_Data", exist_ok=True)

gold = yf.Ticker("GLD")
data = gold.history(period="5y")  # tu peux aussi mettre start/end


print(data.head())


data.to_csv("../data/Raw_Data/gold_prices_yfinance.csv")
print("CSV créé dans ../data/Raw_Data/gold_prices_yfinance.csv")


